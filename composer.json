{
    "name": "raullg/blogged",
    "description": "Write gorgeous articles for your Laravel applications using MarkDown",
    "keywords": ["laravel", "markdown", "blog", "articles"],
    "license": "MIT",
    "type": "library",
    "support": {
        "issues": "https://github.com/saleem-hadad/blogged/issues",
        "source": "https://github.com/saleem-hadad/blogged"
    },
    "authors": [
        {
            "name": "Saleem Hadad",
            "email": "saleem@binary-torch.com"
        }
    ],
    "require": {
        "illuminate/support": "^8.0",
        "erusev/parsedown-extra": "^0.8.1",
        "symfony/dom-crawler": "^4.1|^5.0",
        "mockery/mockery": "^1.3",
        "mews/purifier": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0",
        "laravel/framework": "^8.12",
        "orchestra/database": "^4.0|^5.0|^6.0",
        "orchestra/testbench": "^4.0|^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "BinaryTorch\\Blogged\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BinaryTorch\\Blogged\\Tests\\": "tests/"
      }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "BinaryTorch\\Blogged\\BloggedServiceProvider"
            ]
        }
    }
}
